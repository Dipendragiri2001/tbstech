<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>TBS Technologies | Dashboard</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="~/Admin/plugins/fontawesome-free/css/all.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="~/Admin/https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
  <!-- Tempusdominus Bbootstrap 4 -->~/Admin/
  <link rel="stylesheet" href="~/Admin/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
  <!-- iCheck -->
  <link rel="stylesheet" href="~/Admin/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
  <!-- JQVMap -->
  <link rel="stylesheet" href="~/Admin/plugins/jqvmap/jqvmap.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="~/Admin/dist/css/adminlte.min.css">
  <link href="//cdn.quilljs.com/1.3.6/quill.snow.css" rel="stylesheet">
  <!-- overlayScrollbars -->
  <link rel="stylesheet" href="~/Admin/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
  <!-- Daterange picker -->
  <link rel="stylesheet" href="~/Admin/plugins/daterangepicker/daterangepicker.css">
  <!-- summernote -->
  <link rel="stylesheet" href="~/Admin/plugins/summernote/summernote-bs4.css">
  <link rel="stylesheet" href="~/node_modules/build/toastr.min.css" />
  <!-- Google Font: Source Sans Pro -->
  <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini layout-fixed">
@await Component.InvokeAsync("NToastNotify")
<div class="wrapper">

  <!-- Navbar -->
  <nav class="main-header navbar navbar-expand navbar-white navbar-light">
    <!-- Left navbar links -->
    <ul class="navbar-nav">
      <li class="nav-item d-none d-sm-inline-block">
        <h3>TBS Technology Admin Panel | Only PC View</h3>
      </li>
    </ul>

    <!-- Right navbar links -->
    <ul class="navbar-nav ml-auto">
   
     @await Html.PartialAsync("_LoginPartial")
    </ul>
  </nav>
  <!-- /.navbar -->

  <!-- Main Sidebar Container -->
  <aside class="main-sidebar sidebar-dark-primary elevation-4">
    <!-- Brand Logo -->
    <a href="#" class="brand-link">
      <h3 class="brand-text">TBS Technologies</h3>
    </a>

    <!-- Sidebar -->
    <div class="sidebar">
      <!-- Sidebar user panel (optional) -->
      <div class="user-panel mt-3 pb-3 mb-3 d-flex">
        <div class="image">
          <img src="~/Admin/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
        </div>
        <div class="info">
          <a href="#" class="d-block">Admin</a>
        </div>
      </div>

      <!-- Sidebar Menu -->
      <nav class="mt-2">
        <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
          <!-- Add icons to the links using the .nav-icon class
               with font-awesome or any other icon font library -->
          <li class="nav-item has-treeview ">
            <a asp-controller="Home" asp-action="Index" class="nav-link active">
              <i class="nav-icon fas fa-tachometer-alt"></i>
              <p>
                Dashboard
                
              </p>
            </a>
            
          <li class="nav-item has-treeview">
            <a class="nav-link">
              <i class="nav-icon fab fa-product-hunt"></i>
              <p>
                Product
                <i class="fas fa-angle-left right"></i>
               
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a asp-controller="Product" asp-action="Index" class="nav-link">
                  <p>Show All Products</p>
                </a>
              </li>
              <li class="nav-item">
                <a asp-controller="Product" asp-action="New" class="nav-link">
                  <p>Add New Product</p>
                </a>
              </li>
            </ul>
          </li>
             <li class="nav-item has-treeview">
            <a class="nav-link">
              <i class="nav-icon fab fa-steam-symbol"></i>
              <p>
                Service
                <i class="fas fa-angle-left right"></i>
               
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a asp-controller="Service" asp-action="Index" class="nav-link">
                  
                  <p>Show All Services</p>
                </a>
              </li>
              <li class="nav-item">
                <a asp-controller="Service" asp-action="New" class="nav-link">
                  
                  <p>Add New Service</p>
                </a>
              </li>
            </ul>
          </li>
          <li class="nav-item has-treeview">
            <a class="nav-link">
              <i class="nav-icon fab fa-product-hunt"></i>
              <p>
                Member
                <i class="fas fa-angle-left right"></i>
               
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a asp-controller="Member" asp-action="Index" class="nav-link">
                
                  <p>Show All Members</p>
                </a>
              </li>
              <li class="nav-item">
                <a asp-controller="Member" asp-action="New" class="nav-link">
                 
                  <p>Add New Member</p>
                </a>
              </li>
            </ul>
          </li>
        <li class="nav-item has-treeview">
            <a class="nav-link">
              <i class="nav-icon fab fa-product-hunt"></i>
              <p>
                Course
                <i class="fas fa-angle-left right"></i>
               
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a asp-controller="Course" asp-action="Index" class="nav-link">
                  
                  <p>Show All Courses</p>
                </a>
              </li>
              <li class="nav-item">
                <a asp-controller="Course" asp-action="New" class="nav-link">
                  
                  <p>Add New Course</p>
                </a>
              </li>
               <li class="nav-item">
                <a asp-controller="Course" asp-action="New" class="nav-link">
                  
                  <p>Add New Course</p>
                </a>
              </li>
            </ul>
          </li>
            <li class="nav-item has-treeview">
            <a class="nav-link">
              <i class="nav-icon fab fa-product-hunt"></i>
              <p>
                Video
                <i class="fas fa-angle-left right"></i>
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a asp-controller="Video" asp-action="Index" class="nav-link">
                  <p>Show All Videos</p>
                </a>
              </li>
              <li class="nav-item">
                <a asp-controller="Video" asp-action="New" class="nav-link">
                  <p>Add New Video</p>
                </a>
              </li>
            </ul>
          </li>
          <li class="nav-item has-treeview">
            <a class="nav-link">
              <i class="nav-icon fab fa-product-hunt"></i>
              <p>
                Banner
                <i class="fas fa-angle-left right"></i>
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a asp-controller="Banner" asp-action="Index" class="nav-link">
                  <p>Show Banner</p>
                </a>
              </li>
              <li class="nav-item">
                <a asp-controller="Banner" asp-action="New" class="nav-link">
                  <p>Add Banner</p>
                </a>
              </li>
            </ul>
          </li>
           <li class="nav-item has-treeview">
            <a class="nav-link">
              <i class="nav-icon fab fa-product-hunt"></i>
              <p>
                Contact Details
                <i class="fas fa-angle-left right"></i>
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a asp-controller="ContactDetails" asp-action="Index" class="nav-link">
                  <p>Show Contact Detail</p>
                </a>
              </li>
              <li class="nav-item">
                <a asp-controller="ContactDetails" asp-action="New" class="nav-link">
                  <p>Add Contact Detail</p>
                </a>
              </li>
            </ul>
          </li>
          <li class="nav-item has-treeview">
            <a asp-controller="Password" asp-action="ChangePassword" class="nav-link">
              <i class="nav-icon fab fa-product-hunt"></i>
              <p>
                Password
              </p>
            </a>
            
          </li>
        </ul>
      </nav>
      <!-- /.sidebar-menu -->
    </div>
    <!-- /.sidebar -->
  </aside>


    <div class="content-wrapper container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
    </div>

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
  </aside>
  <!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->

<!-- jQuery -->
<script src="~/Admin/plugins/jquery/jquery.min.js"></script>
<!-- jQuery UI 1.11.4 -->
<script src="~/Admin/plugins/jquery-ui/jquery-ui.min.js"></script>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
  $.widget.bridge('uibutton', $.ui.button)
</script>
<!-- Bootstrap 4 -->
<script src="~/Admin/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- ChartJS -->
<script src="~/Admin/plugins/chart.js/Chart.min.js"></script>
<!-- Sparkline -->
<script src="~/Admin/plugins/sparklines/sparkline.js"></script>
<!-- JQVMap -->
<script src="~/Admin/plugins/jqvmap/jquery.vmap.min.js"></script>
<script src="~/Admin/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
<!-- jQuery Knob Chart -->
<script src="~/Admin/plugins/jquery-knob/jquery.knob.min.js"></script>
<!-- daterangepicker -->
<script src="~/Admin/plugins/moment/moment.min.js"></script>
<script src="~/Admin/plugins/daterangepicker/daterangepicker.js"></script>
<!-- Tempusdominus Bootstrap 4 -->
<script src="~/Admin/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
<!-- Summernote -->
<script src="~/Admin/plugins/summernote/summernote-bs4.min.js"></script>
<!-- overlayScrollbars -->
<script src="~/Admin/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
<!-- AdminLTE App -->
<script src="~/Admin/dist/js/adminlte.js"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<script src="~/Admin/dist/js/pages/dashboard.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="~/Admin/dist/js/demo.js"></script>
<script src="~/node_modules/build/toastr.min.js"></script>
<script src="~/node_modules/dist/jquery.min.js"></script>
<script src="//cdn.quilljs.com/1.3.6/quill.min.js"></script>

@RenderSection("Scripts", required: false)
</body>
</html>
