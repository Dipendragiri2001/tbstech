@inject IServiceRepository service
@{
    var serv = @service.Collection();
}


  
@foreach (var item in serv)

 {  
       <li class="nav-item">
           
          <a asp-action="Service" asp-controller="Home" asp-route-id="@item.Id" class="nav-link">
                      @item.ServiceName
                </a>
       </li>
 }


